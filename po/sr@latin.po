#. to mark text as italic, bold, and monospace.
#.
#: ../xslt/gettext/l10n.xml.in.h:373
msgid "<msgid>glosssee.format</msgid> <msgstr>See <glosssee/>.</msgstr>"
msgstr "<msgid>glosssee.format</msgid> <msgstr>Siehe <glosssee/>.</msgstr>"

